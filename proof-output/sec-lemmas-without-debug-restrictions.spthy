lemma ValidDnssecKeySelectedByRoot [reuse, use_induction,
                                    heuristic={CustomHeuristicValidDnssecKeySelectedByRoot}]:
  all-traces
  "∀ domain root dnssecPublicKey #i #j #k #l.
    ((((DnssecRootCreated( root ) @ #i) ∧
       (SecureChainExtended( domain ) @ #j)) ∧
      (IsApexDomain( domain, root ) @ #k)) ∧
     (IsValidDnssecKeyForDomain( domain, dnssecPublicKey ) @ #l)) ⇒
    (∃ rootOwner #x.
      DnssecPublicKeySelected( root, rootOwner, dnssecPublicKey ) @ #x)"
/*
guarded formula characterizing all counter-examples:
"∃ domain root dnssecPublicKey #i #j #k #l.
  (DnssecRootCreated( root ) @ #i) ∧
  (SecureChainExtended( domain ) @ #j) ∧
  (IsApexDomain( domain, root ) @ #k) ∧
  (IsValidDnssecKeyForDomain( domain, dnssecPublicKey ) @ #l)
 ∧
  ∀ rootOwner #x.
   (DnssecPublicKeySelected( root, rootOwner, dnssecPublicKey ) @ #x) ⇒ ⊥"
*/
induction
  case empty_trace
  by contradiction /* from formulas */
next
  case non_empty_trace
  simplify
  solve( ZoneOwnedBy( 'root', owner ) @ #i )
    case CreateDnssecRoot
    solve( SecureChainExtended( domain ) @ #j )
      case ExtendSecureDnssecChainToNewLabel
      solve( !Parent( domain, parent ) ▶₁ #j )
        case CreateDnsHierarchyRootCase_case_1
