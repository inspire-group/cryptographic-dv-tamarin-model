restriction RegisterSingleCaPerIdentifier:
  "∀ #i #j x.
    ((RegisterSingleCa( x ) @ #i) ∧ (RegisterSingleCa( x ) @ #j)) ⇒
    (#i = #j)"
  // safety formula

restriction RegisterSingleDomainOwnerPerDomain:
  "∀ domain owner1 owner2 #i #j.
    ((ZoneOwnedBy( domain, owner1 ) @ #i) ∧
     (ZoneOwnedBy( domain, owner2 ) @ #j)) ⇒
    (#i = #j)"
  // safety formula

restriction RegisterSingleDnssecKeyPerDomain:
  "∀ #i #j x.
    ((DnssecOptInSelected( x ) @ #i) ∧ (DnssecOptInSelected( x ) @ #j)) ⇒
    (#i = #j)"
  // safety formula

restriction RegisterSingleNsecRequest:
  "∀ domain #i #j.
    ((NsecRequestCreated( domain ) @ #i) ∧
     (NsecRequestCreated( domain ) @ #j)) ⇒
    (#i = #j)"
  // safety formula

restriction SelectSingleValidationMethod:
  "∀ domain domainOwner u1 u2 v1 v2 #i #j.
    ((ValidationMethodSelected( domain, domainOwner, u1, v1 ) @ #i) ∧
     (ValidationMethodSelected( domain, domainOwner, u2, v2 ) @ #j)) ⇒
    (#i = #j)"
  // safety formula

restriction RegisterSingleAdversary:
  "∀ #i #j x y.
    ((AdversaryCreated( x ) @ #i) ∧ (AdversaryCreated( y ) @ #j)) ⇒
    (#i = #j)"
  // safety formula

restriction NoSelfReferencingChildren:
  "¬(∃ x #i. ChildDomainCreated( x, x ) @ #i)"
  // safety formula

restriction NoRootParentDomain:
  "¬(∃ root y #i #j.
      (ChildDomainCreated( root, y ) @ #i) ∧ (DnssecRootCreated( root ) @ #j))"
  // safety formula

restriction SingleParentPerDomain:
  "∀ x y z #i #j.
    ((ChildDomainCreated( x, y ) @ #i) ∧ (ChildDomainCreated( x, z ) @ #j)) ⇒
    (#i = #j)"
  // safety formula

restriction SingleZoneOrLabelCreated:
  "∀ domain #i #j.
    ((ZoneOrLabelCreated( domain ) @ #i) ∧
     (ZoneOrLabelCreated( domain ) @ #j)) ⇒
    (#i = #j)"
  // safety formula

restriction NoDuplicateIdenticalCertIssued:
  "∀ ca domain publicKey #i #j.
    ((CertIssued( ca, domain, publicKey ) @ #i) ∧
     (CertIssued( ca, domain, publicKey ) @ #j)) ⇒
    (#i = #j)"
  // safety formula

restriction SecureChainDomainOwnersDidNotRevealDnssecKey:
  "¬(∃ domain domainOwner #i #j #k.
      ((SecureChainExtended( domain ) @ #i) ∧
       (DnssecKeyRevealed( domain, domainOwner ) @ #j)) ∧
      (ZoneOwnedBy( domain, domainOwner ) @ #k))"
  // safety formula

restriction SingleSecureChainPerDomain:
  "∀ d #x #y.
    ((SecureChainExtended( d ) @ #x) ∧ (SecureChainExtended( d ) @ #y)) ⇒
    (#x = #y)"
  // safety formula

restriction OnlySingleCaaRecordCreatedPerDomainAndOwner:
  "∀ domain domainOwner #i #j.
    ((SignedCaaRecordCreated( domain, domainOwner ) @ #i) ∧
     (SignedCaaRecordCreated( domain, domainOwner ) @ #j)) ⇒
    (#i = #j)"
  // safety formula

restriction Equality:
  "∀ x y #i. (Eq( x, y ) @ #i) ⇒ (x = y)"
  // safety formula

rule (modulo E) CreateSecureCa:
   [ Fr( ~ltk ) ]
  --[
  SecureParticipatingCaCreated( 'ca_secure' ),
  SecureCaCreated( 'ca_secure' ), CaPublicKey( 'ca_secure', pk(~ltk) ),
  RegisterSingleCa( 'ca_secure' )
  ]->
   [
   !CaPrivateKey( 'ca_secure', ~ltk ), !SecureCa( 'ca_secure' ),
   Out( pk(~ltk) )
   ]

