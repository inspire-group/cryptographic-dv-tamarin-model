  all-traces
  "∀ domain zone #i.
    (IsApexDomain( domain, zone ) @ #i) ⇒
    (∃ #x. IsApexDomain( zone, zone ) @ #x)"
/*
guarded formula characterizing all counter-examples:
"∃ domain zone #i.
  (IsApexDomain( domain, zone ) @ #i)
 ∧
  ∀ #x. (IsApexDomain( zone, zone ) @ #x) ⇒ ⊥"
*/
induction
  case empty_trace
  by contradiction /* from formulas */
next
  case non_empty_trace
  simplify
  solve( (last(#i))  ∥
         (∃ #x. (IsApexDomain( zone, zone ) @ #x) ∧ ¬(last(#x))) )
    case case_1
    solve( IsApexDomain( domain, zone ) @ #i )
      case AddLabel
      solve( !Parent( domain, parent ) ▶₀ #i )
        case CreateDnsHierarchyRootCase_case_1
        solve( !ZoneOwner( 'root', zoneOwner ) ▶₂ #i )
          case CreateDnssecRoot
