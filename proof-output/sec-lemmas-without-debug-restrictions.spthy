            case CreateDomain
            solve( !ApexDomain( 'root', zone ) ▶₁ #i )
              case AddRootZone
              by contradiction /* from formulas */
            qed
          qed
        qed
      next
        case CreateDnsHierarchyRootCase_case_2
        solve( !ZoneOwner( 'root', zoneOwner ) ▶₂ #i )
          case CreateDnssecRoot
          by solve( !ZoneOwner( domain, ~DomainOwner ) ▶₃ #i )
        qed
      next
        case CreateDnsHierarchy_case_1
        solve( !ZoneOwner( parent, zoneOwner ) ▶₂ #i )
          case CreateAdversaryDomain
          by solve( !ZoneOwner( domain, ~Adversary ) ▶₃ #i )
        next
          case CreateDnssecRoot
          solve( !ZoneOwner( domain, ~DomainOwner ) ▶₃ #i )
            case CreateDomain
            solve( !ApexDomain( 'root', zone ) ▶₁ #i )
              case AddRootZone
              by contradiction /* from formulas */
            qed
          qed
        next
          case CreateDomain
          solve( !ZoneOwner( domain, ~DomainOwner ) ▶₃ #i )
            case CreateDomain
            solve( !ApexDomain( parent, zone ) ▶₁ #i )
              case AddLabel_case_1
              by contradiction /* from formulas */
            next
              case AddLabel_case_2
              by contradiction /* from formulas */
            next
              case AddLabel_case_3
              by contradiction /* from formulas */
            next
              case AddZone_case_1
              by contradiction /* from formulas */
            next
              case AddZone_case_2
              by contradiction /* from formulas */
            qed
          qed
        qed
      next
        case CreateDnsHierarchy_case_2
        solve( !ZoneOwner( parent, zoneOwner ) ▶₂ #i )
          case CreateAdversaryDomain
          solve( !ZoneOwner( domain, ~Adversary ) ▶₃ #i )
            case CreateAdversaryDomain
            solve( !ApexDomain( parent, zone ) ▶₁ #i )
              case AddLabel
              by contradiction /* from formulas */
            next
              case AddZone_case_1
              by contradiction /* from formulas */
            next
              case AddZone_case_2
              by contradiction /* from formulas */
            qed
          qed
        next
          case CreateDnssecRoot
          by solve( !ZoneOwner( domain, ~DomainOwner ) ▶₃ #i )
        next
          case CreateDomain
          by solve( !ZoneOwner( domain, ~DomainOwner ) ▶₃ #i )
        qed
      qed
    next
      case AddRootZone
      by contradiction /* from formulas */
    next
      case AddZone
      by contradiction /* from formulas */
    qed
  next
    case case_2
    by contradiction /* from formulas */
  qed
