theory CryptographicDomainValidation begin

// Function signature and definition of the equational theory E

functions: fst/1, hsvCriticalFlag/0[private,destructor], pair/2, pk/1,
           sign/2, snd/1, supportDnssecRecordChange/1,
           supportKnownAccountSpecifier/1, true/0,
           validationMethodDnssecRecordChange/0[private,destructor],
           validationMethodHttp/0[private,destructor],
           validationMethodKnownAccountSpecifier/0[private,destructor], verify/3
equations:
    fst(<x.1, x.2>) = x.1,
    snd(<x.1, x.2>) = x.2,
    supportDnssecRecordChange(<dnssecRecordChange, httpValidationOverTls, 
                               knownAccountSpecifier, privateKeyControl, http, noDnssecRecordChange>)
  = dnssecRecordChange,
    supportKnownAccountSpecifier(<dnssecRecordChange, httpValidationOverTls, 
                                  knownAccountSpecifier, privateKeyControl, http, noDnssecRecordChange>)
  = knownAccountSpecifier,
    verify(sign(x.1, x.2), x.1, pk(x.2)) = true

