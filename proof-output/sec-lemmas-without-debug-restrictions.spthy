                              case StartSecureDnssecChain
                              solve( IsApexDomain( 'root', 'root' ) @ #x.1 )
                                case AddRootZone
                                solve( IsApexDomain( 'root', 'root' ) @ #x.2 )
                                  case AddRootZone
                                  solve( !ApexDomain( 'root', 'root' ) ▶₁ #k )
                                    case AddRootZone
                                    solve( !ValidDnssecKeyDelegationSignerOrRoot( 'root', dnssecPublicKey,
                                                                                  parentKeyOrigin
                                           ) ▶₀ #l )
                                      case ValidateDnssecKeyAtRoot
                                      by contradiction /* from formulas */
                                    qed
                                  qed
                                qed
                              qed
                            qed
                          qed
                        qed
                      qed
                    qed
                  next
                    case ValidateDnssecKeyChildDomainWithLabelParent
                    solve( !Parent( domain, parent ) ▶₁ #l )
                      case CreateDnsHierarchyRootCase
                      solve( SecureChainExtended( 'root' ) @ #x )
                        case ExtendSecureDnssecChainToNewLabel
                        by solve( !Parent( 'root', parent ) ▶₁ #x )
                      next
                        case ExtendSecureDnssecChainToNewZone
                        by solve( !Parent( 'root', parent ) ▶₀ #x )
                      next
                        case StartSecureDnssecChain
                        solve( !DnssecRoot( 'root' ) ▶₀ #x )
                          case CreateDnssecRoot
                          solve( !DnssecSecureChainToRoot( 'root' ) ▶₀ #j )
                            case StartSecureDnssecChain
                            solve( !DnssecSecureChainToRoot( 'root' ) ▶₂ #j )
                              case StartSecureDnssecChain
                              solve( IsApexDomain( 'root', 'root' ) @ #x.1 )
                                case AddRootZone
                                solve( IsApexDomain( 'root', 'root' ) @ #x.2 )
                                  case AddRootZone
                                  solve( !ApexDomain( 'root', 'root' ) ▶₁ #k )
                                    case AddRootZone
                                    by solve( !ValidDnssecKeyLabel( 'root', dnssecPublicKey, parentKeyOrigin
                                              ) ▶₀ #l )
                                  qed
                                qed
                              qed
                            qed
                          qed
                        qed
                      qed
                    qed
                  next
                    case ValidateDnssecKeyDelegationSignerWithDsOrRootParent
