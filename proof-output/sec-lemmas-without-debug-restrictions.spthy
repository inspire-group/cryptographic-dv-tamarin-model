                    case ValidateDnssecKeyChildDomainWithDsOrRootParent
                    solve( !Parent( domain, parent ) ▶₁ #l )
                      case CreateDnsHierarchyRootCase
                      solve( SecureChainExtended( 'root' ) @ #x )
                        case ExtendSecureDnssecChainToNewLabel
                        by solve( !Parent( 'root', parent ) ▶₁ #x )
                      next
                        case ExtendSecureDnssecChainToNewZone
                        by solve( !Parent( 'root', parent ) ▶₀ #x )
                      next
                        case StartSecureDnssecChain
                        solve( !DnssecRoot( 'root' ) ▶₀ #x )
                          case CreateDnssecRoot
